#Python

str = "HelloWorld"
length=len(str)
for i in range(length):
    a=ord(str[i]) & 127
    print(chr(a))

for i in range(length):
    a=ord(str[i]) ^ 127
    print(chr(a))
print(a)


#c

#include<stdio.h>
#include<stdlib.h>
void main()
{
char str[]="HelloWorld";
int i , len;
len=strlen(str);
for(i=0;i<len;i++)
{
printf("%c",str[i]&127);
}
printf("\n");
for(i=0;i<len;i++)
{
printf("%c",str[i]^127);
}
printf("\n");
}
